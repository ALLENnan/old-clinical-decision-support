               <item>
                  <content styleCode="Bold">Initial Patient Population =</content>
                  <list>
                     <item>AND: "Patient characteristic: birth date"  &gt;= 23 year(s) starts before start of "Measurement period"</item>
                     <item>AND: "Patient characteristic: birth date"  &lt;= 63 year(s) starts before start of "Measurement period"</item>
