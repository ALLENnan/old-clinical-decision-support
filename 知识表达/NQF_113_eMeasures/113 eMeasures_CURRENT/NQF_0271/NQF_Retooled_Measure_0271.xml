      <section>
         <code code="57026-7" codeSystem="2.16.840.1.113883.6.1" displayName="Population criteria"/>
         <title>Population criteria</title>
         <text>
            <list>
               <item>
                  <content styleCode="Bold">Initial Patient Population =</content>
                  <list>
                     <item>AND: "Patient characteristic: birth date"  &gt;= 18 year(s) starts before start of "Measurement period"</item>
                     <item>AND: "Procedure performed: Non-Cardiac Surgical Procedures requiring Discontinuation of Prophylactic Antibiotics within 24 hours" during "Measurement period"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator =</content>
                  <list>
                     <item>AND: "Initial Patient Population"</item>
                     <item>AND: <list>
                           <item>OR: "Medication administered: Prophylactic antibiotics"  &lt;= 240 minute(s) starts before start of "Procedure performed: Non-Cardiac Surgical Procedures requiring Discontinuation of Prophylactic Antibiotics within 24 hours"</item>
                           <item>OR: "Medication administered: Prophylactic antibiotics" during "Procedure performed: Non-Cardiac Surgical Procedures requiring Discontinuation of Prophylactic Antibiotics within 24 hours"</item>
                           <item> during "Measurement period"</item>
                        </list>
                     </item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Numerator =</content>
                  <list>
                     <item>AND: "Medication administered: Prophylactic antibiotics"  &lt; 24 hour(s) ends after end of ("Procedure performed: Anesthesia administration"  &lt;= 2 hour(s) ends before start of ("Procedure performed: Non-Cardiac Surgical Procedures requiring Discontinuation of Prophylactic Antibiotics within 24 hours" during "Measurement period"))</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Exclusions =</content>
                  <list>
                     <item>OR: "Medication administered: Prophylactic antibiotics (reason: 'is present')"  &gt;= 24 hour(s) ends after end of ("Procedure performed: Anesthesia administration" ends before start of ("Procedure performed: Non-Cardiac Surgical Procedures requiring Discontinuation of Prophylactic Antibiotics within 24 hours" during "Measurement period"))</item>
                  </list>
               </item>
            </list>
