         <title>Population criteria</title>
         <text>
            <list>
               <item>
                  <content styleCode="Bold">Initial Patient Population =</content>
                  <list>
                     <item>AND: "Patient characteristic: birth date"  &gt;= 21 year(s) starts before start of "Measurement period"</item>
                     <item>AND: "Diagnosis active: Osteoarthritis" starts before or during ("Encounter: Encounter Office &amp; Outpatient Consult" during "Measurement period")</item>
                     <item>AND:  &gt;= 2 count(s) of<list>
                           <item>AND: "Encounter: Encounter Office &amp; Outpatient Consult" during "Measurement period"</item>
                        </list>
                     </item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator =</content>
                  <list>
                     <item>AND: "Initial Patient Population"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Numerator =</content>
                  <list>
                     <item>AND: <list>
                           <item>AND: "Intervention performed: Medication Reconciliation"</item>
                           <item>AND: <list>
                                 <item>OR: "Medication active: Over-the-Counter Anti-Inflammatory or Analgesic"</item>
                                 <item>OR: "Medication ordered: Over-the-Counter Anti-Inflammatory or Analgesic"</item>
                              </list>
                           </item>
                           <item> during  ("Encounter: Encounter Office &amp; Outpatient Consult" during "Measurement period")</item>
                        </list>
                     </item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Exclusions =</content>
                  <list>
                     <item>None</item>
                  </list>
               </item>
            </list>
