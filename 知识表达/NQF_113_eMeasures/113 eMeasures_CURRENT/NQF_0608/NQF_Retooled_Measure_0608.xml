      <section>
         <code code="57026-7" codeSystem="2.16.840.1.113883.6.1" displayName="Population criteria"/>
         <title>Population criteria</title>
         <text>
            <list>
               <item>
                  <content styleCode="Bold">Initial Patient Population =</content>
                  <list>
                     <item>AND: "Patient characteristic: birth date"  &gt;= 12 year(s) starts before start of "Measurement end date"</item>
                     <item>AND: "Patient characteristic: Gender Female"</item>
                     <item>AND: "Diagnosis active: Full Term Delivery" during "Measurement period"</item>
                     <item>AND: "Procedure performed: Delivery" during "Measurement period"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator =</content>
                  <list>
                     <item>AND: "Initial Patient Population"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Numerator =</content>
                  <list>
                     <item>AND: "Laboratory test performed: HBsAg"  &lt;= 280 day(s) starts before start of ("Procedure performed: Delivery" during "Measurement period")</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Exclusions =</content>
                  <list>
                     <item>OR: <list>
                           <item>OR: "Diagnosis active: Hepatitis B" starts before start of ("Procedure performed: Delivery" during "Measurement period")</item>
                           <item>OR: "Diagnosis inactive: Hepatitis B" starts before start of ("Procedure performed: Delivery" during "Measurement period")</item>
                        </list>
                     </item>
                  </list>
               </item>
            </list>
         </text><!--
