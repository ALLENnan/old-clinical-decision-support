         <title>Population criteria</title>
         <text>
            <list>
               <item>
                  <content styleCode="Bold">Initial Patient Population =</content>
                  <list>
                     <item>AND: "Patient characteristic: birth date"  &gt;= 18 year(s) starts before start of "Measurement period"</item>
                     <item>AND: "Diagnosis active: Hypertension" starts before or during "Measurement period"</item>
                     <item>AND:  &gt;= 2 count(s) of<list>
                           <item>OR: "Encounter: Encounter Nursing Facility"</item>
                           <item>OR: "Encounter: Encounter Outpatient"</item>
                           <item> during "Measurement period"</item>
                        </list>
                     </item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator =</content>
                  <list>
                     <item>AND: "Initial Patient Population"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Numerator =</content>
                  <list>
                     <item>AND: "Physical exam finding: Systolic Blood Pressure" during <list>
                           <item>OR: "Encounter: Encounter Nursing Facility"</item>
                           <item>OR: "Encounter: Encounter Outpatient"</item>
                           <item> during "Measurement period"</item>
                        </list>
                     </item>
                     <item>AND: "Physical exam finding: Diastolic Blood Pressure" during <list>
                           <item>OR: "Encounter: Encounter Nursing Facility"</item>
                           <item>OR: "Encounter: Encounter Outpatient"</item>
                           <item> during "Measurement period"</item>
                        </list>
                     </item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Exclusions =</content>
                  <list>
                     <item>None</item>
                  </list>
               </item>
            </list>
